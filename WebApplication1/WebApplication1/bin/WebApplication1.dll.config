<configuration>
	<system.webServer>
		<security>
			<requestFiltering >
				<requestLimits maxAllowedContentLength="2147483647" ></requestLimits>
			</requestFiltering>
		</security>
	</system.webServer>
    <system.web>
      <compilation debug="true" targetFramework="4.0" />
	  <httpRuntime maxRequestLength="2097152000" requestValidationMode="2.0" />
    </system.web>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="DCWebServiceConvertSoap" />
            </basicHttpBinding>
            <customBinding>
                <binding name="DCWebServiceConvertSoap12">
                    <textMessageEncoding messageVersion="Soap12" />
                    <httpTransport />
                </binding>
            </customBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:5678/DCWebServiceConvert.asmx"
                binding="basicHttpBinding" bindingConfiguration="DCWebServiceConvertSoap"
                contract="ServiceReference1.DCWebServiceConvertSoap" name="DCWebServiceConvertSoap" />
            <endpoint address="http://localhost:5678/DCWebServiceConvert.asmx"
                binding="customBinding" bindingConfiguration="DCWebServiceConvertSoap12"
                contract="ServiceReference1.DCWebServiceConvertSoap" name="DCWebServiceConvertSoap12" />
        </client>
    </system.serviceModel>
</configuration>
